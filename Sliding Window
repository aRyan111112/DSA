Brute force using nested loop --> TLE

class Solution {
    public int maxSubarraySum(int[] arr, int k) {
        // Code here
        int maxSum = 0;
        for(int i=0; i<=(arr.length-k); i++) {
            int sum=0;
            for(int j=i; j<i+k; j++) {
                sum += arr[j];
                maxSum = Math.max(maxSum, sum);
            }
        }
        return maxSum;
    }
}
